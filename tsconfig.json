{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es6"
  },
  "include": ["src/**/*.tsx"],
  "exclude": ["node_modules"]
}